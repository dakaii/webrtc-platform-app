# LiveKit Configuration
LIVEKIT_API_KEY=your-api-key
LIVEKIT_API_SECRET=your-api-secret
LIVEKIT_HOST=ws://localhost:7880

# Server Configuration
PORT=3000
NODE_ENV=development

# CORS Configuration
CORS_ORIGIN=*

# JWT Configuration
JWT_ISSUER=livekit-app

# Room Configuration
DEFAULT_ROOM_EGRESS_ENABLED=false
DEFAULT_ROOM_MAX_PARTICIPANTS=50
DEFAULT_ROOM_EMPTY_TIMEOUT=300

# Logging
LOG_LEVEL=info